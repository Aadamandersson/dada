async fn main() {
    if 1 + 1 == 2 {
        print("1 + 1 == 2").await
    }

    if 1 - 1 == 0 {
        print("1 - 1 == 0").await
    }

    if 10 * 10 == 100 {
        print("10 * 10 == 100").await
    }

    if 10 / 5 == 2 {
        print("10 / 5 == 2").await
    }

    # round toward 0
    if 11 / 5 == 2 {
        print("11 / 5 == 2").await
    }

    if 1 < 2 {
        print("1 < 2").await
    }

    if 2 < 1 {
        print("error").await
    }

    if 1 < 1 {
        print("error").await
    }

    if 1 > 2 {
        print("error").await
    }

    if 2 > 1 {
        print("2 > 1").await
    }

    if 1 > 1 {
        print("error").await
    }
}
