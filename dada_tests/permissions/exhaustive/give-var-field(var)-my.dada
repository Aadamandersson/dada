class Pair(var a, var b)

async fn main() {
    # Here we have an owned point, but in a shared field
    var p = Pair(Pair(22, 44), 66)

    # Giving that makes an *our* result
    q = p.a.give

    print(p).await #! OUTPUT my Pair\(\(expired\), 66\)
    print(q).await #! OUTPUT our Pair\(22, 44\)
}