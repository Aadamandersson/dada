class Pair(var a, var b)

async fn main() {
    # FIXME: Debatable when the underlying pair should be freed.
    var p = Pair(22, 44).lease
    var q = p.give
    print(p).await #! OUTPUT leased Pair\(22, 44\)
    print(q).await #! OUTPUT leased Pair\(22, 44\)
    print(p).await #! OUTPUT leased Pair\(22, 44\)
}