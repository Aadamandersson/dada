class Point(any a, any b)

async fn main() {
    our p = Point(22, 44)
    our leased q = p
    print(q).await #! OUTPUT Point\(22, 44\)

    p := Point(44, 66)
    q := p
    p := Point(33, 55)
    
    print(p).await #! OUTPUT Point\(33, 55\)
    print(q).await #! RUN ERROR your lease to this object was cancelled
}